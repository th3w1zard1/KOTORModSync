<Project>
    <!-- ensure this is defined, sometimes is ignored in cli -->
    <PropertyGroup>
        <SolutionDir Condition="'$(SolutionDir)'=='' or '$(SolutionDir)'=='*Undefined*'">..\</SolutionDir>
    </PropertyGroup>

    <PropertyGroup>
        <SelfContained>true</SelfContained>
        <TargetFramework>net8.0</TargetFramework>
        <Platform>x64</Platform>
        <RuntimeIdentifier>osx-arm64</RuntimeIdentifier>
        <PublishDir>..\bin\publish\SelfContained\net8.0\osx-arm64\</PublishDir>
        <IncludeNativeLibrariesForSelfExtract>true</IncludeNativeLibrariesForSelfExtract>
        <PublishSingleFile>true</PublishSingleFile>
        <PublishTrimmed>false</PublishTrimmed>
        <_TargetId>Folder</_TargetId>
        <PublishProtocol>FileSystem</PublishProtocol>
        <Configuration>Release</Configuration>
        <UseAppHost>true</UseAppHost>
    </PropertyGroup>
    <PropertyGroup>
        <CFBundleDisplayName>KOTOR Mod Sync</CFBundleDisplayName>
        <CFBundleIdentifier>com.th3w1zard1.kotormodsync</CFBundleIdentifier>
        <CFBundleShortVersionString>v0.10</CFBundleShortVersionString>
        <CFBundleVersion>v0.10.43</CFBundleVersion>
        <LSMinimumSystemVersion>10.13</LSMinimumSystemVersion>
        <CFBundleIconFile>icon53</CFBundleIconFile>
        <CFBundleExecutable>KOTORModSync</CFBundleExecutable>
        <LSApplicationCategoryType>public.app-category.utilities</LSApplicationCategoryType>
    </PropertyGroup>
</Project>